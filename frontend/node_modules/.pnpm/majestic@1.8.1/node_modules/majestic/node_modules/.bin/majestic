#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yangcheng/Desktop/financial_ai_advisor/frontend/node_modules/.pnpm/majestic@1.8.1/node_modules/majestic/dist/server/node_modules:/Users/yangcheng/Desktop/financial_ai_advisor/frontend/node_modules/.pnpm/majestic@1.8.1/node_modules/majestic/dist/node_modules:/Users/yangcheng/Desktop/financial_ai_advisor/frontend/node_modules/.pnpm/majestic@1.8.1/node_modules/majestic/node_modules:/Users/yangcheng/Desktop/financial_ai_advisor/frontend/node_modules/.pnpm/majestic@1.8.1/node_modules:/Users/yangcheng/Desktop/financial_ai_advisor/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yangcheng/Desktop/financial_ai_advisor/frontend/node_modules/.pnpm/majestic@1.8.1/node_modules/majestic/dist/server/node_modules:/Users/yangcheng/Desktop/financial_ai_advisor/frontend/node_modules/.pnpm/majestic@1.8.1/node_modules/majestic/dist/node_modules:/Users/yangcheng/Desktop/financial_ai_advisor/frontend/node_modules/.pnpm/majestic@1.8.1/node_modules/majestic/node_modules:/Users/yangcheng/Desktop/financial_ai_advisor/frontend/node_modules/.pnpm/majestic@1.8.1/node_modules:/Users/yangcheng/Desktop/financial_ai_advisor/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/server/index.js" "$@"
else
  exec node  "$basedir/../../dist/server/index.js" "$@"
fi
